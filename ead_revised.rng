	<!-- Control and inherited models from EAC-CPF -->

	<define name="e.control">
		<element name="control">
			<ref name="a.common"/>
			<optional>
				<attribute name="base">
					<data type="anyURI"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="relatedencoding"/>
			</optional>
			<element name="recordid">
				<ref name="a.common"/>
				<optional>
					<attribute name="instanceurl">
						<data type="anyURI"/>
					</attribute>
				</optional>
				<data type="NMTOKEN"/>
			</element>
			<zeroOrMore>
				<element name="otherrecordid">
					<ref name="a.common"/>
					<ref name="m.id"/>
				</element>
			</zeroOrMore>
			<zeroOrMore>
				<element name="representation">
					<ref name="a.common"/>
					<ref name="am.simplelink"/>
					<optional>
						<ref name="a.localtype"/>
					</optional>
					<text/>
				</element>
			</zeroOrMore>
			<element name="filedesc">
				<ref name="a.common"/>
				<optional>
					<attribute name="encodinganalog"/>
				</optional>
				<element name="titlestmt">
					<ref name="a.common"/>
					<optional>
						<attribute name="encodinganalog"/>
					</optional>
					<oneOrMore>
						<ref name="e.titleproper"/>
					</oneOrMore>
					<zeroOrMore>
						<ref name="e.subtitle"/>
					</zeroOrMore>
					<zeroOrMore>
						<ref name="e.author"/>
					</zeroOrMore>
					<zeroOrMore>
						<ref name="e.sponsor"/>
					</zeroOrMore>
				</element>
				<optional>
					<element name="editionstmt">
						<ref name="a.common"/>
						<optional>
							<attribute name="encodinganalog"/>
						</optional>
						<oneOrMore>
							<choice>
								<ref name="e.edition"/>
								<ref name="e.p"/>
							</choice>
						</oneOrMore>
					</element>
				</optional>
				<optional>
					<element name="publicationstmt">
						<ref name="a.common"/>
						<optional>
							<attribute name="encodinganalog"/>
						</optional>
						<oneOrMore>
							<choice>
								<ref name="e.publisher"/>
								<ref name="e.date"/>
								<ref name="e.address"/>
								<ref name="e.num"/>
								<ref name="e.p"/>
							</choice>
						</oneOrMore>
					</element>
				</optional>
				<optional>
					<element name="seriesstmt">
						<ref name="a.common"/>
						<optional>
							<attribute name="encodinganalog"/>
						</optional>
						<oneOrMore>
							<choice>
								<ref name="e.titleproper"/>
								<ref name="e.num"/>
								<ref name="e.p"/>
							</choice>
						</oneOrMore>
					</element>
				</optional>
				<optional>
					<element name="notestmt">
						<ref name="a.common"/>
						<optional>
							<attribute name="encodinganalog"/>
						</optional>
						<oneOrMore>
							<ref name="e.controlnote"/>
						</oneOrMore>
					</element>
				</optional>
			</element>
			<element name="maintenancestatus">
				<ref name="a.common"/>
				<attribute name="value">
					<choice>
						<value>revised</value>
						<value>deleted</value>
						<value>new</value>
						<value>deletedsplit</value>
						<value>deletedmerged</value>
						<value>deletedreplaced</value>
						<value>cancelled</value>
						<value>derived</value>
					</choice>
				</attribute>
				<ref name="m.mixed.basic"/>
			</element>
			<optional>
				<element name="publicationstatus">
					<ref name="a.common"/>
					<attribute name="value">
						<choice>
							<value>inprocess</value>
							<value>approved</value>
							<value>published</value>
						</choice>
					</attribute>
					<ref name="m.mixed.basic"/>
				</element>
			</optional>
			<ref name="e.maintenanceagency"/>
			<zeroOrMore>
				<element name="languagedeclaration">
					<ref name="a.common"/>
					<ref name="e.language"/>
					<ref name="e.script"/>
					<optional>
						<ref name="e.descriptivenote"/>
					</optional>
				</element>
			</zeroOrMore>
			<zeroOrMore>

				<element name="conventiondeclaration">
					<ref name="a.common"/>
					<optional>
						<ref name="e.abbr"/>
					</optional>
					<ref name="e.citation"/>
					<optional>
						<ref name="e.descriptivenote"/>
					</optional>
				</element>

			</zeroOrMore>
			<zeroOrMore>
				<element name="localtypedeclaration">
					<ref name="a.common"/>
					<optional>
						<ref name="e.abbr"/>
					</optional>
					<ref name="e.citation"/>
					<optional>
						<ref name="e.descriptivenote"/>
					</optional>
				</element>
			</zeroOrMore>
			<zeroOrMore>
				<element name="localcontrol">
					<ref name="a.common"/>
					<attribute name="localtype"/>
					<optional>
						<element name="term">
							<ref name="a.common"/>
							<optional>
								<attribute name="transliteration"/>
							</optional>
							<optional>
								<ref name="a.lastdatetimeverified"/>
							</optional>
							<optional>
								<!-- This source attribute as a data type wherease others don't. Should homogenize. -MR -->
								<attribute name="source">
									<data type="string"/>
								</attribute>
							</optional>
							<optional>
								<attribute name="identifier"/>
							</optional>
							<text/>
						</element>
					</optional>
					<optional>
						<choice>
							<ref name="e.datesingle"/>
							<ref name="e.daterange"/>
						</choice>
					</optional>
				</element>
			</zeroOrMore>
			<element name="maintenancehistory">
				<ref name="a.common"/>
				<oneOrMore>
					<element name="maintenanceevent">
						<ref name="a.common"/>
						<element name="eventtype">
							<ref name="a.common"/>
							<attribute name="value">
								<choice>
									<value>created</value>
									<value>revised</value>
									<value>deleted</value>
									<value>cancelled</value>
									<value>derived</value>
									<value>updated</value>
									<value>unknown</value>
								</choice>
							</attribute>
							<ref name="m.mixed.basic"/>
						</element>
						<element name="eventdatetime">
							<ref name="a.common"/>
							<optional>
								<attribute name="standarddatetime">
									<ref name="m.data.standardDateTime"/>
								</attribute>
							</optional>
							<text/>
						</element>
						<element name="agenttype">
							<ref name="a.common"/>
							<attribute name="value">
								<choice>
									<value>human</value>
									<value>machine</value>
									<value>unknown</value>
								</choice>
							</attribute>
							<ref name="m.mixed.basic"/>
						</element>
						<element a:id="agent" name="agent">
							<ref name="a.common"/>
							<text/>
						</element>
						<optional>
							<element a:id="eventdesccription" name="eventdescription">
								<ref name="a.common"/>
								<text/>
							</element>
						</optional>
					</element>
				</oneOrMore>
			</element>
			<optional>
				<element name="sources">
					<ref name="a.common"/>
					<optional>
						<attribute name="base">
							<data type="anyURI"/>
						</attribute>
					</optional>
					<oneOrMore>
						<element name="source">
							<ref name="a.common"/>
							<optional>
								<ref name="a.lastdatetimeverified"/>
							</optional>
							<optional>
								<ref name="am.simplelink"/>
							</optional>
							<optional>
								<element name="sourceentry">
									<ref name="a.common"/>
									<optional>
										<attribute name="transliteration"/>
									</optional>
									<text/>
								</element>
							</optional>
							<optional>
								<choice>
									<ref name="e.objectxmlwrap"/>
								</choice>
							</optional>
							<optional>
								<ref name="e.descriptivenote"/>
							</optional>
						</element>
					</oneOrMore>
				</element>
			</optional>
		</element>
	</define>

	<define name="m.id">
		<optional>
			<attribute name="localtype"/>
		</optional>
		<text/>
	</define>
	<define name="e.titleproper">
		<element name="titleproper">
			<ref name="a.common"/>
			<optional>
				<attribute name="render">
					<choice>
						<ref name="av.render"/>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="localtype"/>
			</optional>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>

			<ref name="m.mixed.basic"/>

		</element>
	</define>
	<define name="e.subtitle">
		<element name="subtitle">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	<define name="e.author">
		<element name="author">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	<define name="e.sponsor">
		<element name="sponsor">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	<define name="e.edition">
		<element name="edition">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>

			<ref name="m.mixed.basic"/>

		</element>
	</define>
	<define name="e.date">
		<element name="date">
			<ref name="a.common"/>
			<optional>
				<attribute name="localtype"/>
			</optional>
			<ref name="am.dates.era"/>
			<ref name="am.dates.calendar"/>
			<ref name="am.date.normal"/>
			<optional>
				<attribute name="certainty"/>
			</optional>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>

			<ref name="m.mixed.basic"/>

		</element>
	</define>

	<define name="e.publisher">
		<element name="publisher">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>

			<ref name="m.mixed.basic"/>

		</element>
	</define>
	<define name="e.controlnote">
		<element name="controlnote">
			<ref name="a.common"/>
			<optional>
				<attribute name="localtype"/>
			</optional>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>
			<oneOrMore>
				<ref name="m.blocks"/>
			</oneOrMore>
		</element>
	</define>

	<!-- from EAC-CPF -->

	<define name="e.maintenanceagency">
		<element name="maintenanceagency">
			<ref name="a.common"/>
			<optional>
				<element name="agencycode">
					<optional>
						<ref name="a.common"/>
					</optional>
					<text/>
				</element>
			</optional>
			<zeroOrMore>
				<element name="otheragencycode">
					<ref name="a.common"/>
					<attribute name="localtype"/>
					<data type="NMTOKEN"/>
				</element>
			</zeroOrMore>
			<element name="agencyname">
				<text/>
			</element>
			<optional>
				<ref name="e.descriptivenote"/>
			</optional>
		</element>



	</define>

	<define name="e.citation">
		<element a:id="citation" name="citation">
			<ref name="a.common"/>
			<optional>
				<ref name="am.simplelink"/>
			</optional>
			<optional>
				<ref name="a.lastdatetimeverified"/>
			</optional>
			<ref name="m.mixed.basic"/>
		</element>
	</define>

	<define name="e.placeentry">
		<element name="placeentry">
			<ref name="a.common"/>
			<optional>
				<attribute name="transliteration"/>
			</optional>
			<optional>
				<ref name="a.localtype"/>
			</optional>
			<optional>
				<attribute name="countrycode">
					<data type="string"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="vocabularysource"/>
			</optional>
			<optional>
				<attribute name="accuracy">
					<text/>
				</attribute>
			</optional>
			<optional>
				<ref name="m.geographicalcoordinates"/>
			</optional>
			<text/>
		</element>
	</define>

	<define name="m.geographicalcoordinates">
		<attribute name="longitude">
			<data type="NMTOKEN"/>
		</attribute>
		<attribute name="latitude">
			<data type="NMTOKEN"/>
		</attribute>
		<optional>
			<attribute name="altitude">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
	</define>

	<define name="a.lastdatetimeverified">
		<attribute name="lastdatetimeverified">
			<ref name="m.data.standardDateTime"/>
		</attribute>
	</define>

	<define name="m.data.standardDateTime">
		<choice>
			<data type="date">
				<param name="maxInclusive">2099-12-31</param>
			</data>
			<data type="gYear">
				<param name="maxInclusive">2099</param>
			</data>
			<data type="gYearMonth">
				<param name="maxInclusive">2099-12</param>
			</data>
			<data type="dateTime">
				<param name="maxInclusive">2099-12-31T23:59:59</param>
			</data>
		</choice>
	</define>
</grammar>
