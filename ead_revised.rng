	<define name="e.physdescstructured">
		<element name="physdescstructured">
			<ref name="a.common"/>
			<optional>
				<attribute name="label"/>
			</optional>
			<optional>
				<attribute name="encodinganalog"/>
			</optional>
			<optional>
				<attribute name="rules">
					<data type="NMTOKEN"/>
				</attribute>
			</optional>
			<attribute name="physdescstructuredtype">
				<choice>
					<value>carrier</value>
					<value>materialtype</value>
					<value>spaceoccupied</value>
				</choice>
			</attribute>
			<attribute name="coverage">
				<choice>
					<value>whole</value>
					<value>part</value>
				</choice>
			</attribute>

			<element name="quantity">
				<ref name="a.common"/>
				<text/>
			</element>
			<element name="unittype">
				<ref name="a.common"/>
				<optional>
					<attribute name="identifier"/>
				</optional>
				<optional>
					<attribute name="source"/>
				</optional>
				<optional>
					<attribute name="rules">
						<data type="NMTOKEN"/>
					</attribute>
				</optional>
				<text/>
			</element>
			<zeroOrMore>
				<ref name="e.dimensions"/>
			</zeroOrMore>
			<zeroOrMore>
				<ref name="e.physfacet"/>
			</zeroOrMore>
			<optional>
				<ref name="e.descriptivenote"/>
			</optional>
		</element>
	</define>
